#define true 	1
#define false	0

int main(void){
	//Ejemplo 1
	unsigned char age = 0;
	unsigned char elegible;

	if (age < 18){
		elegible = false;
	} else {
		elegible = true;
	}

	//Ejemplo 2

	unsigned char x, y, z, max;
	x = 90;
	y = 52;
	z = 45;

	if (x > y && x > z){
		max = x;
	}else if (z > y && z > x){
		max = z;
	}else{
		max = y;
	}
	return 0;
}


